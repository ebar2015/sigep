	<queryString>
		<![CDATA[Select c.cod_contrato,
       en.codigo_sigep ,en.nombre_entidad,
       per_contrato.primer_nombre ||' '|| per_contrato.segundo_nombre||' '
       ||per_contrato.primer_apellido||' ' ||per_contrato.segundo_apellido nombre_persona_contrato,
       depto.nombre_departamento, municipio.nombre_municipio,
       c.fecha_firma, c.fecha_inicio, c.fecha_fin,
       c.numero_contrato, c.duracion_contrato duracion_dias,
       c.duracion_contrato_meses duracion_meses,
       c.objeto_contrato, c.alcance_contrato,
       pmm.nombre_parametro moneda_monto, pmh.nombre_parametro moneda_honorarios  ,
       c.monto, c.honorarios,
       pff.nombre_parametro fuente_financiacion,
       c.derecho_exclusividad,
       decode(c.derecho_exclusividad,0,'No','Si') derecho_exclusividad_texto
from   contrato c, entidad en, persona per_contrato  ,departamento depto, municipio ,
       parametrica pmm , parametrica pmh, parametrica pff
where  c.cod_contrato =  $P{P_CODIGO_CONTRATO}
  and  c.cod_entidad = en.cod_entidad
  and  c.cod_persona = per_contrato.cod_persona
  and  c.cod_departamento = depto.cod_departamento
  and  c.cod_municipio = municipio.cod_municipio
  and  c.cod_moneda_monto = pmm.COD_TABLA_PARAMETRICA
  and  c.cod_moneda_honorario = pmh.COD_TABLA_PARAMETRICA (+)
  and  c.cod_fuente_financiacion = pff.COD_TABLA_PARAMETRICA (+)]]>
	</queryString>
