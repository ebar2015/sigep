    SELECT RECURSO.COD_RECURSO, RECURSO_ACCION.FLG_ESTADO, RECURSO.CODIGO_VENTANA,RECURSO_ACCION.COD_ROL, RECURSO.ID_BOTON
		FROM   V_RECURSO_ACCION     RECURSO_ACCION,
		       V_PERMISO_ROL_ACCION PERMISO_ACCION,
		       RECURSO              RECURSO,
		       RECURSO              RECURSO_PADRE
		WHERE  RECURSO_ACCION.COD_RECURSO_ACCION = PERMISO_ACCION.COD_RECURSO_ACCION
		  AND  RECURSO_ACCION.COD_PERMISO_ROL = PERMISO_ACCION.COD_PERMISO_ROL
		  AND  RECURSO_ACCION.COD_RECURSO = RECURSO.COD_RECURSO
		   <if test="codRolList != null">
			  AND  RECURSO_ACCION.COD_ROL IN
			  <foreach item="item" index="index" collection="codRolList"
	        			open="(" separator="," close=")">
	          					#{item}
	    	  </foreach>
    	  </if>
		  AND  RECURSO_PADRE.CODIGO_VENTANA = #{codigoVentana,jdbcType=VARCHAR}
		  AND  RECURSO_PADRE.COD_RECURSO = RECURSO.COD_RECURSO_PADRE
  </select>
  
</mapper>