   <select id="selectDenominacionPorNivelNomenclatura" parameterType="co.gov.dafp.sigep2.bean.ext.DenominacionExt" resultMap="BaseResultMapExt">
		select d.COD_DENOMINACION, d.nombre_denominacion,nd.COD_NIVEL_JERARQUICO
		from   denominacion d 
		inner join nomenclatura_denominacion nd on     d.cod_denominacion = nd.cod_denominacion 
		where  1=1
		and	nd.COD_NOMENCLATURA = #{codNomenclatura,jdbcType=NUMERIC} 
		and nd.flg_activo = #{flgActivo,jdbcType=NUMERIC}
		and nd.COD_NIVEL_JERARQUICO =  #{codNivelJerarquico,jdbcType=NUMERIC} 
		group  by d.COD_DENOMINACION, d.nombre_denominacion,nd.COD_NIVEL_JERARQUICO
		order  by d.nombre_denominacion
   </select>
   <select id="selectDenominacionNomenclaturaXEntidad" parameterType="co.gov.dafp.sigep2.bean.ext.DenominacionExt" resultMap="BaseResultMapExt">
		
		select distinct d.COD_DENOMINACION, d.nombre_denominacion,  p.nombre_parametro NIVEL_CARGO, nd.cod_nomenclatura
		from   denominacion d 
		inner join nomenclatura_denominacion nd on     d.cod_denominacion = nd.cod_denominacion 
		inner join nomenclatura n on n.cod_nomenclatura = nd.cod_nomenclatura
		inner join parametrica p on nd.COD_NIVEL_JERARQUICO = p.COD_TABLA_PARAMETRICA
		where  1=1
		and	n.cod_entidad = #{codEntidad,jdbcType=NUMERIC}
		and nd.flg_activo =    1
		group  by d.COD_DENOMINACION, d.nombre_denominacion, p.nombre_parametro, nd.cod_nomenclatura
		order  by d.nombre_denominacion asc
   </select>
