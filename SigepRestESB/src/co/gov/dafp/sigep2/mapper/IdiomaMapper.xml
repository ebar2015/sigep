  <select id="selectIdiomaComplPersona" parameterType="java.lang.Long" resultMap="BaseResultMapExt">
    SELECT
  		i.*,
  		param1.NOMBRE_PARAMETRO  as NVL_CONVERSACION,
  		param2.NOMBRE_PARAMETRO as NVL_REDACCION,
  		param3.nombre_parametro NVL_LECTURA,
  		ip.FLG_IDIOMA_NATIVO as NATIVO
	FROM IDIOMA_PERSONA ip INNER JOIN IDIOMA i ON ip.COD_IDIOMA = i.COD_IDIOMA
  		INNER JOIN PARAMETRICA param1 ON ip.COD_NIVEL_CONVERSACION = param1.COD_TABLA_PARAMETRICA
  		LEFT OUTER JOIN PARAMETRICA param2 ON ip.COD_NIVEL_ESCRITURA = param2.COD_TABLA_PARAMETRICA
		LEFT OUTER JOIN PARAMETRICA param3 on ip.COD_NIVEL_LECTURA = param3.COD_TABLA_PARAMETRICA 
		WHERE ip.COD_PERSONA = #{codPersona,jdbcType=NUMERIC}
		OFFSET 0 ROWS FETCH NEXT 1 ROWS ONLY
  </select>
