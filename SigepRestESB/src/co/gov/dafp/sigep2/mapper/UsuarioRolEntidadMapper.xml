  <select id="selectByEntidadRolPersona" parameterType="java.math.BigDecimal" resultMap="BaseResultMapExt">
    SELECT DISTINCT E.COD_ENTIDAD, URE.COD_ROL
		from usuario_rol_entidad URE
	    INNER JOIN usuario_entidad UE on UE.cod_usuario_entidad = URE.cod_usuario_entidad
	    INNER JOIN ENTIDAD E ON UE.COD_ENTIDAD = E.COD_ENTIDAD
	    INNER JOIN USUARIO U ON U.COD_USUARIO = UE.COD_USUARIO
	    INNER JOIN PERSONA P ON P.COD_PERSONA = U.COD_PERSONA
		WHERE P.COD_PERSONA = #{codPersona,jdbcType=NUMERIC}
		 AND ue.flg_activo = 1
		 AND u.flg_estado = 1
