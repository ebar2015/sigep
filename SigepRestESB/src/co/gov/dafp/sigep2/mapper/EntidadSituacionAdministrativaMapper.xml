   <select id="selectSituacionesAplicadas" parameterType="co.gov.dafp.sigep2.bean.EntidadSituacionAdministrativa" resultMap="BaseResultMapExt">
   SELECT SA.NOMBRE_SITUACION, SA.COD_SITUACION_ADMINISTRATIVA, COUNT(SA.COD_SITUACION_ADMINISTRATIVA) AS NUM_ASIGNADAS
	  FROM ENTIDAD_SITUACION_ADMINISTRATIVA ESA
	    INNER JOIN SITUACION_ADMINISTRATIVA SA ON ESA.COD_SITUACION_ADMINISTRATIVA = SA.COD_SITUACION_ADMINISTRATIVA
	    INNER JOIN ENTIDAD E ON ESA.COD_ENTIDAD = E.COD_ENTIDAD
	    INNER JOIN SITUACION_ADMIN_VINCULACION S on SA.COD_SITUACION_ADMINISTRATIVA = S.COD_SITUACION_ADMINISTRATIVA
	    WHERE ESA.COD_ENTIDAD = #{codEntidad,jdbcType=NUMERIC}
	    AND SA.COD_SITUACION_ADMINISTRATIVA = #{codSituacionAdministrativa,jdbcType=NUMERIC}
	  GROUP BY SA.NOMBRE_SITUACION, SA.COD_SITUACION_ADMINISTRATIVA
	UNION
	SELECT SA.NOMBRE_SITUACION, SA.COD_SITUACION_ADMINISTRATIVA,  0 AS NUM_ASIGNADAS
	  FROM ENTIDAD_SITUACION_ADMINISTRATIVA ESA
	    INNER JOIN SITUACION_ADMINISTRATIVA SA ON ESA.COD_SITUACION_ADMINISTRATIVA = SA.COD_SITUACION_ADMINISTRATIVA
	    INNER JOIN ENTIDAD E ON ESA.COD_ENTIDAD = E.COD_ENTIDAD
	    WHERE ESA.COD_ENTIDAD = #{codEntidad,jdbcType=NUMERIC}
	    AND SA.COD_SITUACION_ADMINISTRATIVA NOT IN(SELECT COD_SITUACION_ADMINISTRATIVA FROM SITUACION_ADMIN_VINCULACION)
	    AND SA.COD_SITUACION_ADMINISTRATIVA = #{codSituacionAdministrativa,jdbcType=NUMERIC}
	  GROUP BY SA.NOMBRE_SITUACION, SA.COD_SITUACION_ADMINISTRATIVA
  </select>
  
