  <select id="selectParametro" parameterType="co.gov.dafp.sigep2.bean.ext.AuditoriaExt" resultMap="BaseResultMapExt">
   SELECT
      a.*,
      p.NOMBRE_PARAMETRO,
       FROM auditoria a
        INNER JOIN Parametrica p ON
                           a.OPERACION = p.COD_TABLA_PARAMETRICA
         INNER JOIN Auditoria_Configuracion ac ON
                                       a.NOMBRE_TABLA = ac.NOMBRE_TABLA
         WHERE p.COD_TABLA_PARAMETRICA= #{codTablaParametrica,jdbcType=NUMERIC}
         OFFSET #{limitInit} ROWS FETCH NEXT #{limitEnd} ROWS ONLY
  </select>
  <select id="selectFechas" parameterType="co.gov.dafp.sigep2.bean.ext.AuditoriaExt" resultMap="BaseResultMapExt">
   SELECT 
   count(*) over() TOTAL,
   a.*,
   concat(pe.primer_nombre || ' ', pe.primer_apellido) AS nombre_persona,
   p.NOMBRE_PARAMETRO, rol.nombre NOMBRE_ROL, ptipoident.nombre_parametro NOMBRE_TIPO_IDENTIFICACION, pe.NUMERO_IDENTIFICACION NUMERO_IDENTIFICACION
   FROM auditoria a
   INNER JOIN Parametrica p ON a.OPERACION = p.COD_TABLA_PARAMETRICA
   INNER JOIN usuario usr ON a.aud_cod_usuario = usr.cod_usuario
   INNER JOIN persona pe ON usr.cod_persona = pe.cod_persona
   INNER JOIN rol rol on a.aud_cod_rol = rol.cod_rol
   inner join parametrica ptipoident on ptipoident.cod_tabla_parametrica = pe.cod_tipo_identificacion
   WHERE 1=1
   <if test="codTablaParametrica != null and codTablaParametrica != 0">
     and a.OPERACION = #{codTablaParametrica,jdbcType=NUMERIC}
    </if>
   and  AUD_FECHA_MODIFICACION BETWEEN #{fechaIni,jdbcType=TIMESTAMP} AND  #{fechaFin,jdbcType=TIMESTAMP} + 1
   AND  a.NOMBRE_TABLA = #{nombreTabla,jdbcType=VARCHAR}
   order by AUD_FECHA_MODIFICACION desc
   OFFSET #{limitInit,jdbcType=NUMERIC} ROWS FETCH NEXT #{limitEnd,jdbcType=NUMERIC} ROWS ONLY
  </select>
  
